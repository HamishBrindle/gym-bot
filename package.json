{
  "name": "gym-bot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "deploy": "npm run aws:build && npm run aws:deploy",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "generate": "graphql-codegen",
    "seed": "ts-node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm-seeding/dist/cli.js seed -n ./ormconfig.js",
    "seed:reset": "rm db/line.sqlite && npm run seed",
    "aws:login": "aws ecr get-login-password --region us-west-1 | docker login --username AWS --password-stdin 834491810670.dkr.ecr.us-west-1.amazonaws.com",
    "aws:build": "npm run aws:login && npm run docker:build && npm run docker:tag && npm run docker:push",
    "aws:deploy": "aws ecs update-service --cluster gym-bot-cluster --service gym-bot-service --force-new-deployment --region=us-west-1",
    "docker:build": "docker build -t gym-bot .",
    "docker:tag": "docker tag gym-bot:latest 834491810670.dkr.ecr.us-west-1.amazonaws.com/gym-bot:latest",
    "docker:push": "docker push 834491810670.dkr.ecr.us-west-1.amazonaws.com/gym-bot:latest"
  },
  "dependencies": {
    "@nestjs/bull": "^0.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/typeorm": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "bull": "^3.14.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "cron-parser": "^2.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "mysql": "^2.18.1",
    "node-cron": "^2.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^5.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.32",
    "@types/bull": "^3.14.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.12",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/node-cron": "^2.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/puppeteer": "^3.0.0",
    "@types/redis": "^2.8.22",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsdoc": "^27.1.2",
    "jest": "26.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
